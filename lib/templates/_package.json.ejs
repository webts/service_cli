{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description":"generated package.json",
  "license": "Commercial",
  "dependencies": {
  <% npm.forEach(function(p,idx){ %>
    "<%= p %>":"latest"<% if(idx < npm.length-1) {%>,<%}%>
  <% }); %>
  },
  "scripts": {
    "clean": "rm -rf build",
    "transpile": "./node_modules/.bin/babel ./app -d ./build --copy-files",
    "build": "yarn clean && yarn transpile",
    "start": "if [ ${NODE_ENV} = \"production\" ]; then ./node_modules/.bin/pm2-docker start app.config.js; else node app.js; fi"
  }
}