{
  "name": "micro-webservice-cli",
  "version": "0.0.9yar",
  "description": "CLI tool for microservice projects",
  "main": "index.js",
  "repository": "git@github.com:webts/service-cli.git",
  "author": "Minh Tran(gtdminh@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0",
    "cpy": "^6.0.0",
    "docker-compose": "^0.4.3",
    "ejs": "^2.5.7",
    "globby": "^7.1.1",
    "immutable": "^3.8.2",
    "js-yaml": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-transform-object-assign": "latest",
    "@babel/polyfill": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-flow": "latest",
    "@babel/preset-react": "latest",
    "@babel/runtime": "latest",
    "babel-7-jest": "^21.3.2",
    "jest": "latest",
    "object-assign": "^4.1.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "clean": "rimraf  dist/**",
    "transpile": "babel ./lib -d ./dist --copy-files --include-dotfiles --source-maps",
    "build": "npm run clean && npm run transpile",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "bin": {
    "service_cli": "dist/cli.js"
  },
  "jest":{
    "transform":{
      ".js$":"babel-7-jest"
    },
    "roots":["./lib/","./tests/"]
  }
}
