{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description":"generated package.json",
  "license": "Commercial",
  "dependencies": {
    "micro-webservice":"latest",
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/polyfill":"latest",
    "@babel/plugin-transform-object-assign": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@babel/preset-flow":"latest",
    "@babel/runtime": "latest",
    "object-assign": "^4.1.1",
    "pm2":"latest"<% if(npm && npm.length > 0){%>,<%}%>
  <% npm.forEach(function(p){ %>
    "<%= p %>":"latest",
  <% }); %>
  },
  "scripts": {
    "clean": "rm -rf build",
    "transpile": "babel ./app -d ./build --copy-files",
    "build": "yarn clean && yarn transpile",
    "start": "if [[ ${NODE_ENV} = production ]]; then ./node_modules/.bin/pm2-docker start app.config.js; else ./node_modules/.bin/pm2-dev app.config.js; fi"
  }
}